{
  "authors": [
    {
      "email": "thomscode@gmail.com",
      "name": "Thom Williams"
    }
  ],
  "autoload": {
    "psr-4": {
      "pnnl\\Composer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pnnl\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "conflict": {
    "drupal/core": "8.*"
  },
  "description": "Composer installer for Drupal modules, themes, etc",
  "extra": {
    "class": "pnnl\\Composer\\DrupalInstallerPlugin"
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "pnnl/drupal-installer",
  "prefer-stable": true,
  "require": {
    "composer-plugin-api": "^1.1",
    "php": "^5.6||^7.0"
  },
  "require-dev": {
    "composer/composer": "^1.5",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "nikic/php-parser": "^3.1",
    "php-coveralls/php-coveralls": "^2.0",
    "phpmd/phpmd": "^2.6",
    "phpro/grumphp": "^0.14.0",
    "phpunit/phpunit": "^5.7",
    "sensiolabs/security-checker": "^4.1",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "test": "phpunit"
  },
  "type": "composer-plugin"
}
