{
  "authors": [
    {
      "email": "thomscode@gmail.com",
      "name": "Thom Williams"
    }
  ],
  "autoload": {
    "psr-4": {
      "pnnl\\Composer\\": "src",
      "pnnl\\Tests\\": "tests"
    }
  },
  "conflict": {
    "drupal/core": "8.*"
  },
  "description": "Composer installer for Drupal modules, themes, etc",
  "extra": {
    "class": "pnnl\\Composer\\DrupalInstallerPlugin"
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "pnnl/drupal-installer",
  "prefer-stable": true,
  "require": {
    "composer-plugin-api": "^1.1",
    "php": "^5.6||^7.0"
  },
  "require-dev": {
    "composer/composer": "^1.5",
    "php-coveralls/php-coveralls": "^2.0",
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "test": "phpunit"
  },
  "type": "composer-plugin"
}
